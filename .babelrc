{
    "presets": [
        [
            "@babel/preset-env",
            {
                "useBuiltIns": "usage",
                "targets": {
                    "edge": "17",
                    "firefox": "60",
                    "chrome": "58",
                    "safari": "11.1",
                    "ie": "10"
                }
            }
        ],
        "@babel/preset-react",
        "@babel/preset-typescript",
    ],
    "plugins": [
        [
            "@babel/plugin-transform-runtime"
        ],
        "@babel/plugin-syntax-dynamic-import"
    ]
}